<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/background"
    android:id="@+id/teamFragment"
    >

    <TextView
        android:id = "@+id/text_team"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="@string/text_team"
        android:textColor="?attr/text_color"
        android:layout_marginStart="@dimen/margin_16"
        android:layout_marginTop="@dimen/margin_19"
        app:textFontWeight="500"
        android:textSize="@dimen/text_22sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id = "@+id/text_worked_application"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="@string/text_developer_worker"
        android:textColor="?attr/text_color"
        android:layout_marginStart="@dimen/margin_16"
        android:layout_marginTop="@dimen/margin_43"
        android:textFontWeight="700"
        android:textSize="@dimen/text_32sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_team"/>

    <TextView
        android:id = "@+id/text_developer_name_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="@string/text_developer_name_first"
        android:textColor="?attr/text_color"
        android:layout_marginStart="@dimen/margin_16"
        android:layout_marginTop="@dimen/margin_32"
        android:textFontWeight="500"
        android:textSize="@dimen/text_16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_worked_application"
        />

    <TextView
        android:id = "@+id/text_developer_name_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="@string/text_developer_name_second"
        android:textColor="?attr/text_color"
        android:layout_marginStart="@dimen/margin_16"
        android:layout_marginTop="@dimen/margin_16"
        android:textFontWeight="500"
        android:textSize="@dimen/text_16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_developer_name_first"
        />

    <TextView
        android:id = "@+id/text_developer_name_third"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="@string/text_developer_name_third"
        android:textColor="?attr/text_color"
        android:layout_marginStart="@dimen/margin_16"
        android:layout_marginTop="@dimen/margin_16"
        android:textFontWeight="500"
        android:textSize="@dimen/text_16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_developer_name_second"
        />
    <TextView
        android:id = "@+id/text_developer_name_four"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/ys_display_medium"
        android:text="@string/text_developer_name_four"
        android:textColor="?attr/text_color"
        android:layout_marginStart="@dimen/margin_16"
        android:layout_marginTop="@dimen/margin_16"
        android:textFontWeight="500"
        android:textSize="@dimen/text_16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/text_developer_name_third"
        />

</androidx.constraintlayout.widget.ConstraintLayout>