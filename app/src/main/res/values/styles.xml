<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="filterEditText">
        <item name="android:layout_height">@dimen/margin_60</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">104dp</item>
        <item name="android:paddingStart">@dimen/margin_16</item>
        <item name="android:paddingEnd">@dimen/margin_24</item>
        <item name="android:inputType">text</item>
        <item name="android:textSize">@dimen/text_16sp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:hint">@string/job</item>
        <item name="android:textColorHint">@color/gray</item>
        <item name="android:background">@null</item>
        <item name="android:drawableEnd">@drawable/arrow_forward</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="bottom_nav_text">
        <item name="android:textSize">@dimen/text_12sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
</resources>